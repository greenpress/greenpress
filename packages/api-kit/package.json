{
   "name": "@greenpress/api-kit",
   "version": "2.0.15",
   "description": "API-Kit package to help with greenpress infrastructure and reuse capabilities across services",
   "main": "src/server.ts",
   "repository": {
      "type": "git",
      "url": "git+https://github.com/greenpress/greenoress.git"
   },
