    "passport-local": "^1.0.0",
    "validator": "^7.0.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.23",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/passport": "^1.0.6",
    "@types/passport-local": "^1.0.33",
